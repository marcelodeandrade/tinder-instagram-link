{
    "manifest_version": 2,
    "name": "My new extension",
    "description": "It does stuff",
    "version": "1",
    "content_scripts": [
        {
            "matches": ["https://tinder.com/app/recs/*"],
            "js": ["src/content.script.js"]
        }
    ],
    "permissions": [
        "tabs", "https://tinder.com/app/recs/*"
    ]
}