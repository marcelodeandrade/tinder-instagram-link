{
    "manifest_version": 2,
    "name": "Tinder web - Instagram Link",
    "description": "Allows intasgram view profile",
    "version": "1",
    "icons": {
        "120": "assets/instagram.png"
    },
    "content_scripts": [
        {
            "matches": ["https://tinder.com/app/recs/*"],
            "js": ["src/content.script.js"]
        }
    ],
    "permissions": [
        "tabs", "https://tinder.com/app/recs/*"
    ]
}